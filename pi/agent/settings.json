  "packages": [
    "extensions/filter-output.ts",
    "extensions/uv.ts",
    "extensions/nightshift.ts",
    "extensions/ship.ts",
    {
      "source": "npm:mitsupi",
      "extensions": [
        "!uv.ts"
      ]
    },
    "npm:checkpoint"
